<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds.  Do not translate. -->

<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
  
  <!-- For Ultra Power Saving -->
  <bool name="config_supportUltraPowerSaving">true</bool> 
 
  <!-- Battery Health -->
  <string name="config_batDesCap">/sys/class/power_supply/bms/charge_full</string>
  <string name="config_batCurCap">/sys/class/power_supply/bms/charge_now_raw</string>
  <string name="config_batChargeCycle">/sys/class/power_supply/bms/cycle_count</string>
  <bool name="config_supportBatteryHealth">true</bool>
 

  <!-- Whether SELinux status should be shown or not. -->
  <bool name="config_show_selinux_status">true</bool>

  <!-- Whether to show Connected MAC Randomization in Developer Options
       as not all devices can support dynamic MAC address change.  -->
  <bool name="config_wifi_support_connected_mac_randomization">true</bool>

  <!-- Enable Manual Sub Provisioning -->
  <bool name="config_enableManualSubProvisioning">true</bool>

  <!-- Legion About -->
  <string name="legion_device_name_summary">ZenFone Max Pro M1</string>
  <string name="legion_chipset_type_summary">Qualcomm Snapdragon 636</string>
  <string name="legion_gpu_summary">Adreno 509</string>
  <string name="legion_camera_mp_summary">(13/5)(16/5)(8/16)</string>
  <string name="legion_screen_type_summary">IPS LCD 5.99</string>

</resources>
